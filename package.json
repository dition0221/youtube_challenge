{
  "name": "youtube_challenge",
  "version": "1.0.0",
  "description": "풀스택으로 Youtube와 비슷한 웹 사이트를 구성합니다. Challenge. YouTube Clone built using NodeJS, Express, MongoDB, Mongoose and ES6.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dition0221/youtube_challenge.git"
  },
  "scripts": {
    "start": "node build/init.js",
    "build": "npm run build:server && npm run build:assets",
    "build:server": "babel src -d build --ignore src/client",
    "build:assets": "webpack --mode=production",
    "dev:server": "nodemon",
    "dev:assets": "webpack --mode=development -w"
  },
  "author": "dition0221",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dition0221/youtube_challenge/issues"
  },
  "homepage": "https://github.com/dition0221/youtube_challenge#readme",
  "dependencies": {
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.6",
    "bcrypt": "^5.1.0",
    "connect-mongo": "^5.0.0",
    "cross-fetch": "^3.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.3",
    "mongoose": "^7.3.0",
    "multer": "^1.4.5-lts.1",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "@flydotio/dockerfile": "^0.3.3",
    "@prettier/plugin-pug": "^2.5.1",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
    "mini-css-extract-plugin": "^2.7.6",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4"
  }
}
