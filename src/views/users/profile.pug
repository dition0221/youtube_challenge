extends ../base
include ../mixins/video

block content
  .profile__data
    if isFlyio
      img.avatar(src=user.avatarUrl, crossorigin)
    else
      img.avatar(src="/" + user.avatarUrl, crossorigin)
    h3= user.name
  .video-grid
    each video in videos
      +video(video) 
    else
      span.empty__message No Videos found.
